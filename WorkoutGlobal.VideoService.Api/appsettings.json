    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDbConnection": {
    "DatabaseName": "VideoServiceDb"
  },
  "ConnectionStrings": {
